ainer elementor-column-gap-default\">\n\t\t\t\t\t\t\t<div class=\"elementor-row\">\n\t\t\t\t\t<div class=\"elementor-column elementor-col-100 elementor-top-column elementor-element elementor-element-zgcucgn\" data-id=\"zgcucgn\" data-element_type=\"column\">\n\t\t\t<div class=\"elementor-column-wrap elementor-element-populated\">\n\t\t\t\t\t\t\t<div class=\"elementor-widget-wrap\">\n\t\t\t\t\t\t<div class=\"elementor-element elementor-element-kpqpufl elementor-widget elementor-widget-rt-image-carousel\" data-id=\"kpqpufl\" data-element_type=\"widget\" data-widget_type=\"rt-image-carousel.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\r\n\t\t\t\t<div id=\"image-carousel-595301\" class=\"rt-carousel carousel-holder clearfix rt-image-carousel  rt_lightbox_gallery\" data-item-width=\"5\" data-nav=\"true\" data-dots=\"\" data-margin=\"0\" data-autoplay=\"\" data-timeout=\"\" data-thumbnails=\"false\" data-boxed=\"\" data-min-height=\"\" data-padding=\"0\" data-loop=\"\" data-tablet-item-width=\"3\" data-mobile-item-width=\"2\">\r\n\t\t\t\t\t<div class=\"owl-carousel\">\r\n\t\t\t\t\t\t<div><img loading=\"lazy\" src=\"https:\/\/icassessments.com\/wp-content\/uploads\/2020\/07\/2.png\" width=\"125\" height=\"50\" alt=\"\" srcset=\"https:\/\/icassessments.com\/wp-content\/uploads\/2020\/07\/2.png 125w, https:\/\/icassessments.com\/wp-content\/uploads\/2020\/07\/2-62x25.png 62w\" sizes=\"https:\/\/icassessments.com\/wp-content\/uploads\/2020\/07\/2.png 125w, https:\/\/icassessments.com\/wp-content\/uploads\/2020\/07\/2-62x25.png 62w\"><\/div><div><img loading=\"lazy\" src=\"https:\/\/icassessments.com\/wp-content\/uploads\/2020\/07\/3.jpg\" width=\"400\" height=\"59\" alt=\"\" srcset=\"https:\/\/icassessments.com\/wp-content\/uploads\/2020\/07\/3.jpg 400w, https:\/\/icassessments.com\/wp-content\/uploads\/2020\/07\/3-300x44.jpg 300w, https:\/\/icassessments.com\/wp-content\/uploads\/2020\/07\/3-200x29.jpg 200w\" sizes=\"https:\/\/icassessments.com\/wp-